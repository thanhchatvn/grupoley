<odoo>
  <data>
    <record id="report_stock_quantity_custom" model="ir.ui.view">
            <field name="name">report.stock.quantity.custom</field>
            <field name="model">report.stock.quantity</field>
            <field name="inherit_id" ref="stock_enterprise.stock_report_view_grid"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="after">
                    <field name="x_product_category" type="row"/>
                </xpath>
            </field>
        </record>
  </data>
</odoo>