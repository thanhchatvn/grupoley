<odoo>
  <data>
      <record id="stock_picking_form_remis" model="ir.ui.view">
          <field name="name">stock.picking.form</field>
          <field name="model">stock.picking</field>
          <field name="inherit_id" ref="stock.view_picking_form"/>
          <field name="arch" type="xml">
              <xpath expr="//field[@name='x_order_folio']" position="after">
                  <field name="x_remis"/>
              </xpath>
          </field>
      </record>

  </data>
</odoo>