<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

      <!-- Transfer  wizard -->
      <act_window
            id="print_transfer_report_action"
            name="Transfer Report by Specific Period"
            binding_model="stock.internal.transfer"
            res_model="filter.transfer"
            view_mode="form"
            target="new"
          />

      <!-- wizard view -->
        <record model="ir.ui.view" id="project_details_wizard_view">
            <field name="name">filter.transfer</field>
            <field name="model">filter.transfer</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="from_date"/>
                        </group>
                        <group>
                            <field name="to_date"/>
                        </group>
                    </group>

                    <footer>
                        <button name="print_report" string="Print Report" 
                          type="object" default_focus="1" class="oe_highlight" icon="fa-print"/>
                        <button string="Cancel" class="btn-default" special="cancel" icon="fa-times"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>